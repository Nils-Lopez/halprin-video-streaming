  "scripts": {
    "prisma:migrate": "dotenv-flow -- yarn prisma migrate",
    "prisma:migrate:reset": "dotenv-flow -- yarn prisma migrate reset",
    "prisma:db:seed": "dotenv-flow -- yarn prisma db seed",
    "prisma:db:push": "dotenv-flow -- yarn prisma db push",
